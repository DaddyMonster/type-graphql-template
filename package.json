    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "graphql-redis-subscriptions": "^2.3.1",
    "graphql-upload": "^11.0.0",
    "ioredis": "^4.22.0",
    "mongoose": "^5.10.17",
    "pg": "^8.5.1",
